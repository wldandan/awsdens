<h1>Listing artifact_types</h1>

<table>
  <tr>
    <th>Clinic</th>
    <th>Name</th>
    <th>Price</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @artifact_types.each do |artifact_type| %>
  <tr>
    <td><%= artifact_type.clinic_id %></td>
    <td><%= artifact_type.name %></td>
    <td><%= artifact_type.price %></td>
    <td><%= artifact_type.description %></td>
    <td><%= link_to 'Show', artifact_type %></td>
    <td><%= link_to 'Edit', edit_artifact_type_path(artifact_type) %></td>
    <td><%= link_to 'Destroy', artifact_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

